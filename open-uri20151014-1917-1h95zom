<script type="text/javascript">
	var multisearchurls = [
		'/%EC%B9%BC%EB%A1%9C%EB%A6%AC-%EC%98%81%EC%96%91%EC%86%8C/search?q=',
		'/Default.aspx?pa=rs&recipe=',
